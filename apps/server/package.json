{
    "name": "@glb/server",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "main": "dist/main.js",
    "scripts": {
        "dev": "tsx watch src/main.ts",
        "build": "tsc",
        "prebuild:cache": "node scripts/prebuild/applist-v8cache.js",
        "prebuild:snapshot": "node --build-snapshot scripts/prebuild/applist-snapshot.js && mv snapshot.blob data/",
        "start": "node dist/main.js",
        "start:snapshot": "node --snapshot-blob data/snapshot.blob dist/main.js",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@fastify/cors": "11.1.0",
        "fastify": "5.6.0",
        "zod": "4.1.5"
    },
    "devDependencies": {
        "pino-pretty": "13.1.1",
        "tsx": "4.20.5",
        "typescript": "5.9.2"
    }
}
